<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Detail', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>